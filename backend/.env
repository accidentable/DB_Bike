# /ddareungi-backend/.env

# Node.js 서버 설정
NODE_ENV=development
PORT=3000

# JWT 시크릿 키 (아무도 모르게 복잡하게 변경하세요)
JWT_SECRET_KEY=my_super_secret_key

# DB 연결 정보 (docker-compose.yml과 Node.js 앱이 함께 사용)
DB_USER=myadmin
DB_PASSWORD=mypassword123
DB_NAME=ddareungi
DB_HOST=localhost  # 내 앱은 localhost:5432로 접속
DB_PORT=5432